<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="userController" class="com.spring.iocxml.autowire.controller.UserController" autowire="byType"></bean>
    <bean id="userService" class="com.spring.iocxml.autowire.service.UserServiceImpl"  autowire="byType"></bean>
    <bean id="userDao" class="com.spring.iocxml.autowire.dao.UserDaoImpl"></bean>
</beans>