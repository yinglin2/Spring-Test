<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!--1. set方法注入-->
    <bean id="book" class="com.spring.iocxml.di.Book">
        <property name="bname" value="Frontend Develop"></property>
        <property name="author" value="ying"></property>
    </bean>

    <!--2. 构造去注入-->
    <bean id="bookCon" class="com.spring.iocxml.di.Book">
        <constructor-arg name="bname" value="java develop"></constructor-arg>
        <constructor-arg name="author" value="ying"></constructor-arg>
    </bean>
</beans>